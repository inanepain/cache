{
    "name": "inanepain/cache",
    "description": "Some simple caching tools implementing PSR-6 and PSR-16.",
    "type": "library",
    "license": "UNLICENSE",
    "homepage": "http://vaieen.local:3000/inane/cache",
    "readme": "README.adoc",
    "authors": [
        {
            "name": "Philip Michael Raab",
            "email": "peep@inane.co.za",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "peep@inane.co.za",
        "issues": "http://vaieen.local:3000/iane"
    },
    "keywords": [
        "inane",
        "cache"
    ],
    "autoload": {
        "psr-4": {
            "Inane\\Cache\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "discard-changes": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "psr/cache": ">= 3",
        "psr/simple-cache": ">= 3",
        "inanepain/stdlib": ">=0.1.5 || dev-master || dev-develop",
        "inanepain/file": ">= 0.2.1 || dev-master || dev-develop"
    },
    "require-dev": {}
}
